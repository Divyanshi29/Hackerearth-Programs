#include<stdio.h>
#define MAX 100
int main()
{
int a[MAX],i,j,temp,n,xchanges;
printf("enter the no of element : " );
scanf("%d",&n);
for(i=0;i<n;i++)
{
   printf("enter element %d : ",i+1);
   scanf("%d",&a[i]);
   }
     for(i=0;i<n-1;i++)
     { 
     xchanges=0;
     for(j=0;j<n=1;j++)
     {
        if(a[j]>a[j+1])
        {
        temp=a[j];
        a[j]=a[j+1];
        a[j+1]=temp;
        xchanges++;
        }
        if(xchanges==0)
        break;
        }
        printf("sorted list : \n");
        for(i=0;i<n;i++)
             printf("%d",a[i]);
             printf("\n");
             return 0;
             }
